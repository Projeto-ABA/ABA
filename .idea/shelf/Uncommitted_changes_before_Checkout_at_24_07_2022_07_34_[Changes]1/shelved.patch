+    public void editar(InstrutorDTO instrutorDTO) {
+        this.nome = instrutorDTO.getNome();
+        this.idade = instrutorDTO.getIdade();
+        this.email = instrutorDTO.getEmail();
+    }
+
+    public InstrutorDTO getDto() {
